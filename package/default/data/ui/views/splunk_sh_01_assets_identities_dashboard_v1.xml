<form theme="dark" script="tabs.js" stylesheet="tabs.css">
  <label>SH - 01 - GMC Assets &amp; Identities - Dashboard - v1.5.15</label>
  <search id="splunk_sh_01_assets_identities_dashboard_tab1">
    <query>| from lookup:splunk_assets_kv_store_lookup
| fields Splunk_Instance Splunk_Roles serverName host host_short host_fqdn host_resolved host_ip idxcluster_label shcluster_label mode guid version build os_name os_name_extended cpu_arch os_version os_build startup_time kvStoreStatus numberOfCores numberOfVirtualCores physicalMemoryMB rtsearch_enabled master_uri master_guid licenseState isForwarding isTrial license_labels transparent_hugepages_defrag transparent_hugepages_effective_state transparent_hugepages_enabled ulimits_core_file_size ulimits_cpu_time ulimits_data_file_size ulimits_data_segment_size ulimits_nice ulimits_open_files ulimits_resident_memory_size ulimits_stack_size ulimits_user_processes ulimits_virtual_address_space_size SPLUNK_DB SPLUNK_HOME appServerPorts dfsDisabled dfsMasterPort enableSplunkWebSSL httpport kvStoreDisabled kvStorePort mgmtHostPort minFreeSpace sessionTimeout startwebserver trustedIP
| eval isForwarding = if (match(Splunk_Roles, "indexer"), "N/A", isForwarding)
| fillnull value="Undefined" idxcluster_label shcluster_label 
| search Splunk_Instance="*$Splunk_Instance$*" Splunk_Roles="*$Splunk_Roles$*" os_name="$os_name$" version="$version$" isForwarding="$isForwarding$" kvStoreDisabled="$kvStoreDisabled$" enableSplunkWebSSL="$enableSplunkWebSSL$" startwebserver="$startwebserver$" idxcluster_label="$idxcluster_label$" shcluster_label="$shcluster_label$" guid="*$guid$*"</query>
  </search>
  <search id="splunk_sh_01_assets_identities_dashboard_tab2">
    <query>| from lookup:splunk_identities_kv_store_lookup
| fields shcluster_label,identity,prefix,nick,first,last,suffix,email,phone,phone2,managedBy,priority,bunit,category,watchlist,startDate,endDate,work_city,work_country,work_lat,work_long,work_region,emp_cc,emp_dep,emp_floor,emp_lob,emp_loc,emp_name,emp_status,emp_ttl,emp_type,emp_tz,Splunk_Instance,splunk_role_map,ldap_group_name,ldap_group_type,last_successful_login
| `get_instance_roles(Splunk_Instance)`
| `get_splunk_roles_info(shcluster_label,splunk_role_map)`
| fillnull value="undefined" shcluster_label Splunk_Instance Splunk_Roles identity emp_type emp_status work_city work_region work_country emp_loc emp_dep emp_lob emp_cc emp_name splunk_role_map ldap_group_name ldap_group_type last_successful_login
| search shcluster_label="$shcluster_label$" Splunk_Roles="$Splunk_Roles$" identity="*$identity$*" emp_type="$emp_type$" emp_status="$emp_status$" work_city="$work_city$" work_region="$work_region$" work_country="$work_country$" emp_loc="$emp_loc$" emp_dep="$emp_dep$" emp_lob="$emp_lob$" emp_cc="$emp_cc$" emp_name="$emp_name$" Splunk_Instance="$Splunk_Instance$" splunk_role_map="$splunk_role_map$" ldap_group_name="$ldap_group_name$" ldap_group_type="$ldap_group_type$" last_successful_login="$last_successful_login$"</query>
  </search>
