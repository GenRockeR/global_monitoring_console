  <row id="Assets_Panel2">
    <panel>
      <title>4. Splunk SHC Captains</title>
      <table>
        <search base="splunk_sh_01_assets_identities_dashboard_tab1">
          <query>| stats values(Splunk_Roles) AS Splunk_Roles by Splunk_Instance  
| where Splunk_Roles="shc_captain"
| eval Splunk_Roles = mvfilter(match(Splunk_Roles,"shc_captain|SHC"))
| eval Splunk_Instance=upper(Splunk_Instance)
| table Splunk_Roles Splunk_Instance</query>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">none</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">false</option>
        <format type="color" field="Splunk_Instance">
          <colorPalette type="sharedList"></colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
      </table>
    </panel>
    <panel>
      <title>5. Instances By Splunk Version</title>
      <chart>
        <search base="splunk_sh_01_assets_identities_dashboard_tab1">
          <query>| stats count by version</query>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.chart.sliceCollapsingThreshold">0</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.legend.placement">right</option>
        <option name="refresh.display">none</option>
      </chart>
    </panel>
    <panel>
      <title>6. Instances By Forwarding Status</title>
