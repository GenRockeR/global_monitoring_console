| convert ctime(startup_time) 
| eval Memory = round (physicalMemoryMB / 1024, 2) 
| table Splunk_Instance Splunk_Roles idxcluster_label shcluster_label host_fqdn guid host_short host_ip version numberOfCores numberOfVirtualCores Memory os_name os_version startup_time Time_Since_Startup_Time isForwarding ulimits_open_files ulimits_stack_size ulimits_user_processes transparent_hugepages_defrag transparent_hugepages_effective_state transparent_hugepages_enabled appServerPorts dfsDisabled dfsMasterPort enableSplunkWebSSL httpport kvStoreDisabled kvStorePort mgmtHostPort minFreeSpace sessionTimeout startwebserver trustedIP SPLUNK_DB SPLUNK_HOME 
| `rename_common_fields` 
| `rename_instance_fields`</query>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">false</option>
        <format type="number" field="Memory">
