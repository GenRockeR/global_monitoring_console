          <scale type="sharedCategory"></scale>
        </format>
        <format type="color" field="IDX Cluster Name">
          <colorPalette type="sharedList"></colorPalette>
          <scale type="sharedCategory"></scale>
        </format>
        <format type="color" field="Instance FQDN">
